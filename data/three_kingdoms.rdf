        [ a foaf:character ;
            foaf:age "61岁" ;
            foaf:name "姜维" ] ;
    foaf:location "洮阳" ;
    foaf:note "演义称夏侯霸于蜀汉景耀五年（262）战死于洮阳，不见于史。夏侯霸卒年，史无明文。据《三国志》有关传记推断，他病死于蜀汉景耀二年（259）。" ;
    foaf:time "262年" .

