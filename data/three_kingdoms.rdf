        [ a foaf:character ;
            foaf:age "58岁" ;
            foaf:name "羊祜" ] ;
    foaf:location "雒阳" ;
    foaf:note "None" ;
    foaf:time "278年" .

<http://www.example.com/羊祜送药与陆抗> a foaf:event ;
    foaf:chapter "第120回第5段" ;
    foaf:description "一天，陆抗遣人问候羊祜。羊祜询问来人陆抗的身体情况。来人说陆抗卧病在床，已经几天没有出来了。羊祜把自己熬成的药交给来人，让他送给陆抗。来人拿着羊祜的药面见陆抗，众将都说此药有问题。陆抗深信不疑，立即仰面服下。第二天，陆抗病愈，众将都拜贺。" ;
    foaf:event_name "羊祜送药与陆抗" ;
    foaf:history "一次陆抗生病，向羊祜求药，羊祜马上派人把药送过来，并说：“这是我最近自己配制的药，还未服，听说您病了，就先送给您吃。”吴将怕其中有诈，劝陆抗勿服，陆抗不疑，并说：“羊祜岂鸩人者”（《晋书·羊祜传》）！仰而服下。" ;
